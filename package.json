{
  "name": "gemini-vision-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for image analysis using Gemini 2.5 Pro through OpenRouter",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gemini",
    "vision",
    "image-analysis",
    "openrouter"
  ],
  "author": "Progressus Software Ltd.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stanley-marketing/gemini-vision-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/stanley-marketing/gemini-vision-mcp/issues"
  },
  "homepage": "https://github.com/stanley-marketing/gemini-vision-mcp#readme",
  "sideEffects": false,
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}